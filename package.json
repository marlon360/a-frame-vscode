		"update-docs": "node ./parser/parser.js"
	},
	"devDependencies": {
		"@types/node": "^10.12.18",
		"aframe": "^0.9.1",
		"markdown-json": "^1.4.0",
		"tabletojson": "^1.0.4",
		"tslint": "^5.11.1",
		"turndown": "^5.0.3",
		"typescript": "^3.2.4",
		"vscode": "^1.1.27"
	}
}