{
	"name": "a-frame-completion",
	"displayName": "A-Frame Completion",
	"version": "0.2.1",
	"publisher": "mlueckert",
	"icon": "images/logo.jpg",
	"galleryBanner": {
		"color": "#24C9FF",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/marlon360/a-frame-vscode.git"
	},
	"engines": {
		"vscode": "^1.18.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
